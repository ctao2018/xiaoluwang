      <div class="ola-date">确认剩余时间：<i class="global-timeout" data-date="1534660181000"></i></div>
      <div class="ola-btn">
      <% for(var i=0;i<btn.length;i++){ %>
        <button type="button" class="btn-normal"><%= btn[i] %></button>
      <% } %>
      </div>
    </div>
    <% }else{ %>
    <div class="ol-act">
      <% for(var i=0;i<btn.length;i++){ %>
        <button type="button" class="btn-normal"><%= btn[i] %></button>
      <% } %>
    </div>
    <% } %>
  <% } %>
