<% if(isHead){ %>
<div class="nav-wrap <%= headerBg %>">
  <% if(ltBtn1 != false){ %>
  <div class="lt-btn-box">
    <% if(ltBtn1 != false){ %>
      <button type="button" class="<%= ltBtn1 %>"><%= ltStr1 %></button>
    <% } %>
    <% if(ltBtn2 != false){ %>
      <button type="button" class="<%= ltBtn2 %>"><%= ltStr2 %></button>
    <% } %>
  </div>
  <% } %>
  <% if(showTitle){ %>
    <h3 class="nav-title"><%= pageTitle %></h3>
  <% } %>
  <% if(navList){ %>
    <h3 class="nav-title show-navlist"><span><%= pageTitle %></span><i class="slide"></i></h3>
    <ul class="nav-list">
      <li class="active"><%= pageTitle %></li>
      <li>选项2</li>
      <li>选项3</li>
    </ul>
  <% } %>
  <% if(search != false){ %>
    <div class="<%= search %>"><input type="text" placeholder="<%= searchStr %>"></div>
  <% } %>
  <% if(rtBtn1 != false){ %>
    <div class="rt-btn-box">
      <% if(rtBtn1 != false){ %>
        <button type="button" class="<%= rtBtn1 %>"><%= rtStr1 %></button>
      <% } %>
      <% if(rtBtn2 != false){ %>
        <button type="button" class="<%= rtBtn2 %>"><%= rtStr2 %></button>
      <% } %>
    </div>
    <% } %>
</div>
<% } %>

