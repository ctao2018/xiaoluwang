
@import './base'
.my-points
  .mw-box
    margin-top Rem(-45)
    getBg("mywalletBg")
    background-size 100% auto
    // height Rem(200)
    .mwb-page
      padding-top Rem(45)
      height Rem(155)
      .myp-box
        display flex
        justify-content space-between
        align-items: center
        color: #fff
        margin-top Rem(25)
        font-size: Rem(13)
        .myp-left
          width: 55%
          text-align: center
          .myp-moneys
            font-size: Rem(28)
            margin-bottom Rem(8)
        .myp-right
          width 45%
          text-align left
          >p
            padding-bottom: Rem(8)
    .mpn-section-box
      border-top-left-radius: Rem(10)
      border-top-right-radius: Rem(10)
      overflow hidden
      margin-top Rem(-10)
      .mpn-section
        background-color: #fff
        .mpn-list
          display flex
          justify-content: space-between
          align-items: center
          padding Rem(15) 0
          botLine()
          color #999
          p
            width 25%
            text-align: center
            font-size: Rem(25/2)
        .mpn-title
          color: #333
         