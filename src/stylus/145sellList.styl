@import './base'

.sell-list
    .sl-top
        height Rem(40)
        line-height: Rem(40)
        padding 0 Rem(15)
        font-size Rem(15)
        color #333333
        background-color #ffffff
        margin-bottom Rem(10)
    .sl-box
        background-color #ffffff
        >a
            padding 0 Rem(15)
            height Rem(50)
            line-height Rem(50)
            color #666666
            font-size Rem(15)
            botLine()
            display block