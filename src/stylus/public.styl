@import './base'

@keyframes emersion
  0%
    opacity 0
    transform translateY(Rem(15))
  100%
    opacity 1
    transform translateY(Rem(0))

@keyframes unemersion
  0%
    opacity 1
    transform translateY(Rem(0))
  100%
    opacity 0
    transform translateY(Rem(15))

[data-tabbox]
  display none