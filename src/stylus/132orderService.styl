@import './base'

.order-service
    .os-menu-list
      background-color: #fff
      font-size: 0
      border-bottom 1px solid #e6e6e6
      li
          font-size: Rem(14)
          display: inline-block
          width 25%
          text-align: center
          a
              padding: Rem(10) 0
              display: inline-block
              color #666
          .pr-on
              border-bottom 1px solid $theme-color
              color $theme-color