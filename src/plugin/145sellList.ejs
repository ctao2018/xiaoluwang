<script>

  
    // 上拉加载html拼接方法
    function getHtmlTemp(res) {
      var _html ='<a href="javascript:void(0)">'+ res.name +'</a>'
      
      return _html;
    }
  
    $(function () {
      // 初始化上拉加载
      onReachBottom.init('.sl-box',function () {
        $.ajax({
          url: './front/mockdata/145sellList.json',
          type: 'GET',
          success: function (res) {
            var pages = res.pages;
            for (var i = 0; i < pages.length; i++) {
              $(".sl-box .show-reach-loading").before(getHtmlTemp(pages[i]))
            }
            hideReachLoading();
            onReachBottom.hasNextPage = res.hasNextPage;//获取是否有下一页数据
            onReachBottom.currPage = res.currPage;//获取当前页数
            if(!onReachBottom.hasNextPage){
              showNullReach($(".sl-box"))
            }
          }
        })
      })
    })
  </script>
    