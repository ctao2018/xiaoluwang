<script>
    $(function(){
            // 点击顶部选择
        $(".pa-menu-list").find('li').each(function(i){
            $(this).on("click",function(){
                $(".pa-menu-list li a").removeClass("pr-on");
                $(this).find("a").addClass("pr-on");
                $('.pa-big').find('.pa-box').eq(i).show().siblings().hide();
            })
        })

    })
    

</script>