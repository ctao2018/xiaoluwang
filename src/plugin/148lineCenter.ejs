<script>
    $(function(){
            // 点击顶部选择
        $(".lc-menu-list").find('li').each(function(){
            $(".lc-menu-list li").on("click",function(){
                $(".lna-menu-list li a").removeClass("pr-on")
                $(this).find("a").addClass("pr-on");
            })
        })
   
    })
</script>